{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"plugins": [
		"@typescript-eslint"
	],
	"ignorePatterns": [
		"*.cjs"
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2016
	},
	"env": {
		"browser": true,
		"es2016": true
	}
}